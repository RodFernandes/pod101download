{
  "manifest_version": 2,
  "name": "Pod101Donwload",
  "description": "Pod 101 Donwload",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon_16x16.png",
    "default_popup": "index.html",
    "default_title": "Download content Pod 101"
  }
}
